<h5></h5>

<%= form_tag sessions_path do %>
    <% flash.each do |key, msg| %>
        <div><%= msg %></div>
    <% end %>


<form>
  <div class="form-group">
    <%= label_tag :user_name %>
    <%= text_field_tag :user_name, params[:user_name] %>
  </div>

  <div class="form-group">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
    <%= submit_tag "Log in" %>



<% end %>
